<div class="carousel55">


    <img class="image" src="/img/item2.png" alt="Image 1">
   

    <img class="image" src="/img/item1.png" alt="Image 2">
    <img class="image" src="/img/item3.png" alt="Image 3">
  </div>
  <script>
    const carousel55 = document.querySelector('.carousel55');
const images = carousel55.querySelectorAll('.image');
let currentImageIndex = 0;

function rotateImages() {
  images[currentImageIndex].style.display = 'none';
  currentImageIndex = (currentImageIndex + 1) % images.length;
  images[currentImageIndex].style.display = 'block';
}

setInterval(rotateImages, 5000); // Change the interval as needed
  </script>
  